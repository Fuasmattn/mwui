{
  "name": "@fuasmattn/mwui",
  "version": "0.0.14",
  "description": "Component Library using figma design tokens",
  "main": "index.ts",
  "browser": "library/dist/index.esm.js",
  "types": "library/dist/index.d.ts",
  "files": [
    "library/dist"
  ],
  "workspaces": [
    "token-farm",
    "library"
  ],
  "engines": {
    "npm": ">=8.7.0"
  },
  "scripts": {
