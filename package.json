  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.0"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.3"
  }
}
